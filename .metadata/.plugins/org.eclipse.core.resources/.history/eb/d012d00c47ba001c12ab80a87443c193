package org.com.main;

public class Student {
	//data members
	public String name;
	public String id;
	public String email;
	public String phoneN;
	public String assN;
	public String assName;
	public String date;
	public String profName;
	public String courseName;
	public String courseNum;
	
	Student(String name, String id,	 String email,	 String phoneN,	 String assN,	 String assName,	 String date,	 String profName,	 String courseName,	 String courseNum){
		this.name=name;
		this.id=id;
		this.email=email;
		this.phoneN=phoneN;
		this.assN = assN;
		this.assName = assName;
		this.date = date;
		this.profName = profName;
		this.courseName =courseName;
		this.courseNum= courseNum;
	}
}
